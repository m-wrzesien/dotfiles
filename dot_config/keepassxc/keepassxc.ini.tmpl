[General]
ConfigVersion=2
GlobalAutoTypeKey=0
GlobalAutoTypeModifiers=0
HideWindowOnCopy=false
MinimizeOnCopy=true
UpdateCheckMessageShown=true

[Browser]
CustomProxyLocation=
Enabled=true

[FdoSecrets]
Enabled=false

[GUI]
ApplicationTheme=dark
CheckForUpdates=false
HidePasswords=true
HideUsernames=false
TrayIconAppearance=monochrome-light

[KeeShare]
Active="<?xml version=\"1.0\"?><KeeShare><Active/></KeeShare>\n"
Own="<?xml version=\"1.0\"?><KeeShare><PrivateKey/><PublicKey/></KeeShare>\n"

[PasswordGenerator]
AdditionalChars=
AdvancedMode=true
ExcludeAlike=true
ExcludedChars=
Length=24
Logograms=true
LowerCase=true
Math=false
SpecialChars=false
UpperCase=true

{{ if eq .type "private" -}}
[SSHAgent]
Enabled=true
{{- end }}

[Security]
AutotypeAsk=false
IconDownloadFallback=true
