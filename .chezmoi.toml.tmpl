{{- $email := promptStringOnce . "email" "Email address" -}}
{{- $github_noreply_email := promptStringOnce . "github_noreply_email" "GitHub noreply email address" -}}
{{- $type := promptStringOnce . "type" "PC type (work/private)" "private" -}}


[data]
    email = {{ $email | quote }}
    github_noreply_email = {{ $github_noreply_email | quote }}
    type = {{ $type | quote }}